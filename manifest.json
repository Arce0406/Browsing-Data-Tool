{
    "name": "Browsing Data Tool",
    "description": "Site session、cookies、local storage...",
    "manifest_version": 3,
    "version": "0.0.0.1",
    "author": "Arce0406",
    "permissions": [
        "notifications",
        "tabCapture",
        "activeTab",
        "tabs",
        "scripting",
        "storage",
        "contextMenus",
        "tabGroups"
    ],
    "options_ui": {
        "page": "./views/options.html",
        "open_in_tab": true
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "",
        "default_icon": {
            "16": "/images/16.png",
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
        }
    },
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    }
}